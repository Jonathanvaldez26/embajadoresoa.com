ue=\"Actualizar\" id=\"btn-actualizar\" name=\"btn-actualizar\" class=\"btn btn-primary btn-modern\" data-loading-text=\"Enviando...\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t\t\t</form>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t\t\t\t<div class=\"card-body\">\n\t\t\t\t\t\t\t\t\t\t<div class=\"col-lg-10\" style=\"margin: 0 auto;\">\n\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</section>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<?php } ?>\n\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t\t\t<!-- end: page -->\n\t\t\t\t</section>\n\t\t\t</div>\n\n\t\t</section>\n\n\t\t<!-- Vendor -->\n\t\t<script type=\"text/javascript\">\n\t\t\t\n\t\t\t$(function() {\n      \t\t\t$(\"#btn-registro\").click(function(){\n         \t\tif($(\"#laboratorio\").val()!=\"\" && $(\"#prefijo\").val()!=\"\" && $(\"#becas\").val()!=\"\" && $(\"#usuario\").val()!=\"\" && $(\"#contrasena\").val()!=\"\" && $(\"#correo\").val()!=\"\"){\n\n            if(validarEmail($(\"#correo\").val())){              \n                registrar();                                           \n              }else{\n                  alert(\"Por favor ingresa un correo valido\");\n                  $(\"#correo\").focus();\n                }\n            }else{\n              alert(\"Todos los campos son obligatorios\");\n              $(\"#laboratorio\").focus();\n              $(\"#prefijo\").focus();\n              $(\"#becas\").focus();\n              $(\"#usuario\").focus();\n              $(\"#contrasena\").focus();\n              $(\"#correo\").focus();\n            }\n          });\n      });\t\t\t\n\n    \tfunction registrar(){\n      $('#modal').modal('show');\n      $.ajax({\n          url: 'guardar_becas.php',\n          type: 'POST',\n          dataType: 'json',\n          data: {\n            laboratorio: $(\"#laboratorio\").val().toUpperCase(),\n            prefijo: $(\"#prefijo\").val().toUpperCase(),\n            becas: $(\"#becas\").val(),\n            usuario: $(\"#usuario\").val(),\n            contrasena: $(\"#contrasena\").val(),\n            correo: $(\"#correo\").val(),\n            id_curso: $(\"#id_curso\").val(),\n            lab: $(\"#lab\").val()\n          }\n      })\n      .done(function(json) {\n\n         if(json.id_laboratorio!=0){\n            $('#modal').modal('hide');\n            alert(json.mensaje);\n            enviarcorreo(json.id_laboratorio);\n            window.location.href = json.redireccion;            \n         }else{\n            alert(json.mensaje);\n         }\n            \n      })\n\n      .fail(function() {\n         alert(\"Ups! Ha ocurrido un error, intente de nuevo\");\n      })\n\n      .always(function() {\n         $('#modal').modal('hide');\n      });\n   }\n\n\n   $(function() {\n  \t\t$(\"#btn-actualizar\").click(function(){\n     \t\tif($(\"#becas2\").val()!=\"\" && $(\"#id_curso2\").val()!=\"\"){            \n            \tactualizarbeca();                                                    \t\n        \t}else{\n          alert(\"Todos los campos son obligatorios\");          \n          $(\"#becas2\").focus();\n          $(\"#id_curso2\").focus();\n        }\n      });\n      });\n\n   function actualizarbeca(){\n      $('#modal').modal('show');\n      $.ajax({\n          url: 'editar_becas.php',\n          type: 'POST',\n          dataType: 'json',\n          data: {\n            id_laboratorio2: $(\"#id_laboratorio2\").val(),            \n            becas2: $(\"#becas2\").val(),\n            id_curso2: $(\"#id_curso2\").val()            \n          }\n      })\n      .done(function(json) {\n\n         if(json.succes){\n            $('#modal').modal('hide');\n            alert(json.mensaje);            \n            window.location.href = json.redireccion;           \n         }else{\n            alert(json.mensaje);\n            window.location.href = json.redireccion; \n         }\n            \n      })\n\n      .fail(function() {\n         alert(\"Ups! Ha ocurrido un error, intente de nuevo\");\n      })\n\n      .always(function() {\n         $('#modal').modal('hide');\n      });\n   }\n\n   function enviarcorreo(id_laboratorio){\n    $.ajax({\n          url: 'enviar_correo_labs.php',\n          type: 'POST',\n          dataType: 'json',\n          data: { id_laboratorio:id_laboratorio }\n      })\n\n    // .fail(function() {\n    //      alert(\"Error al enviar correo\");\n    //   })\n   }\n   <?php if (isset($_GET['lab'])) { ?>\n   function validarbecas() {\n    var id_curso = $('#id_curso2').val();\n    var id_laboratorio = <?php echo base64_decode($_GET['lab']); ?>;\n    $.ajax({\n      url: 'cambiar_becas.php',\n      type: 'POST',\n      dataType: 'json',\n      data: {id_curso:id_curso,id_laboratorio:id_laboratorio},\n\n    })\n    .done(function(json) {\n        if(json.success){\n          $(\"#becas2\").val(json.becas);\n          $(\"#becas2\").attr(\"min\",json.becas);\n\n        }\n      })\n\n  }\n\t<?php } ?>\n\n   function validarEmail(email) {\n      var re = /^([\\w-]+(?:\\.[\\w-]+)*)@((?:[\\w-]+\\.)*\\w[\\w-]{0,66})\\.([a-z]{2,6}(?:\\.[a-z]{2})?)$/i;\n      return re.test(email);\n   }\n\n\t\t</script>\n\t\t<script src=\"vendor/jquery/jquery.js\"></script>\n\t\t<script src=\"vendor/jquery-browser-mobile/jquery.browser.mobile.js\"></script>\n\t\t<script src=\"vendor/popper/umd/popper.min.js\"></script>\n\t\t<script src=\"vendor/bootstrap/js/bootstrap.js\"></script>\n\t\t<script src=\"vendor/bootstrap-datepicker/js/bootstrap-datepicker.js\"></script>\n\t\t<script src=\"vendor/common/common.js\"></script>\n\t\t<script src=\"vendor/nanoscroller/nanoscroller.js\"></script>\n\t\t<script src=\"vendor/magnific-popup/jquery.magnific-popup.js\"></script>\n\t\t<script src=\"vendor/jquery-placeholder/jquery.placeholder.js\"></script>\n\t\t\n\t\t<!-- Specific Page V